{
  "manifest_version": 3,
  "name": "Autobot",
  "version": "1.0",
  "description": "Expand your network, unlock your career: LinkedIn automation with AI",
  "permissions": ["storage", "identity", "tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "host_permissions": [
    "http://localhost/*"
  ]
}
